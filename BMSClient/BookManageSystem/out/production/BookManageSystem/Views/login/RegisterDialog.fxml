<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@RegDialog.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.login.RegisterDialogController">
   <children>
      <Label fx:id="headImg" layoutX="42.0" layoutY="126.0" prefHeight="96.0" prefWidth="96.0" />
      <VBox layoutX="163.0" layoutY="33.0" prefHeight="282.0" prefWidth="344.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="用户名" />
                  <JFXTextField fx:id="NameField" prefHeight="24.0" prefWidth="180.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="密码" />
                  <JFXPasswordField fx:id="PwdField" prefHeight="24.0" prefWidth="180.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="再次输入" />
                  <JFXPasswordField fx:id="DPwdField" prefHeight="24.0" prefWidth="180.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="198.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="邮箱" />
                  <JFXTextField fx:id="EmailField" prefHeight="24.0" prefWidth="180.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="221.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="手机号" />
                  <JFXTextField fx:id="TeleField" prefHeight="24.0" prefWidth="180.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="235.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0">
               <children>
                  <Label prefWidth="60.0" text="职业" />
                  <JFXTextField fx:id="JobField" prefWidth="180.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <JFXButton fx:id="enterButton" layoutX="196.0" layoutY="335.0" onMouseClicked="#onRegButtonClicked" prefWidth="70.0" text="注册" />
      <JFXButton fx:id="regButton" layoutX="335.0" layoutY="335.0" onMouseClicked="#onQuitButtonClicked" prefWidth="70.0" text="退出" />
   </children>
</AnchorPane>
